<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.fx.SignInUpController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="159.0" layoutY="36.0" prefHeight="328.0" prefWidth="282.0">
            <children>
                <Label fx:id="layout" alignment="CENTER" prefHeight="67.0" prefWidth="286.0" text="Sign Up">
                    <font>
                        <Font name="Segoe UI" size="16.0" />
                    </font>
                </Label>
                <TextField fx:id="fullName" layoutX="10.0" layoutY="92.0" promptText="full name" style="-fx-background-radius: 10;" />
                <TextField fx:id="organization" layoutX="10.0" layoutY="92.0" promptText="organisation" style="-fx-background-radius: 10;">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin></TextField>
                <TextField promptText="em@il" style="-fx-background-radius: 10;" fx:id="emailField">
                    <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                    </VBox.margin>
                </TextField>
                <PasswordField promptText="password" style="-fx-background-radius: 10;" fx:id="passField">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </PasswordField>
                <PasswordField fx:id="rePassField" layoutX="10.0" layoutY="128.0" promptText="re-enter password" style="-fx-background-radius: 10;" />
                <Button mnemonicParsing="false" onAction="#signUp" prefHeight="26.0" prefWidth="117.0" style="-fx-background-color: #647dee; -fx-text-fill: #ffffff;                          -fx-background-radius: 10;" text="Sign Up">
                    <VBox.margin>
                        <Insets top="30.0" />
                    </VBox.margin></Button>
                <Button mnemonicParsing="false" onAction="#switchToSignIn" prefHeight="26.0" prefWidth="112.0" style="-fx-text-fill: #b2b8b8; -fx-background-color: transparent;" text="Sign In">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Segoe UI" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
